<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
    <base>CRM/Memberrenewallog</base>
    <class>MembershipRenewalLog</class>
    <name>civicrm_membershiprenewallog</name>
    <comment>FIXME</comment>
    <add>4.4</add>
    <log>true</log>

    <field>
        <name>id</name>
        <type>int unsigned</type>
        <required>true</required>
        <comment>Unique MembershipRenewalLog ID</comment>
        <add>4.4</add>
    </field>
    <primaryKey>
        <name>id</name>
        <autoincrement>true</autoincrement>
    </primaryKey>
    <field>
        <name>contact_id</name>
        <type>int unsigned</type>
        <comment>FK to Contact</comment>
        <add>4.4</add>
    </field>
    <foreignKey>
        <name>contact_id</name>
        <table>civicrm_contact</table>
        <key>id</key>
        <add>4.4</add>
        <onDelete>CASCADE</onDelete>
    </foreignKey>
    <field>
        <name>membership_id</name>
        <type>int unsigned</type>
        <comment>FK to Membership</comment>
        <add>4.4</add>
    </field>
    <foreignKey>
        <name>membership_id</name>
        <table>civicrm_membership</table>
        <key>id</key>
        <add>4.4</add>
        <onDelete>CASCADE</onDelete>
    </foreignKey>
    <field>
        <name>start_date</name>
        <type>date</type>
        <required>true</required>
        <comment>Renewal Start Date</comment>
        <add>4.4</add>
    </field>
    <field>
        <name>end_date</name>
        <type>date</type>
        <required>true</required>
        <comment>Renewal End Date</comment>
        <add>4.4</add>
    </field>
    <field>
        <name>contribution_id</name>
        <type>int unsigned</type>
        <comment>FK to Contribution</comment>
        <add>4.4</add>
    </field>
    <foreignKey>
        <name>contribution_id</name>
        <table>civicrm_contribution</table>
        <key>id</key>
        <add>4.4</add>
        <onDelete>CASCADE</onDelete>
    </foreignKey>
    <field>
        <name>modified_date</name>
        <type>date</type>
        <required>true</required>
        <comment>Modified Date</comment>
        <add>4.4</add>
    </field>

</table>
